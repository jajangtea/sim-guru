                                <label class="col-md-2 control-label">&nbsp;</label>
                                <div class="col-md-12">
                                    <com:TActiveButton ID="btnUpdate" CssClass="btn btn-primary float-right" ValidationGroup="Editsekolah" Onclick="updateData">                       
                                        <prop:Text>                            
                                            Update
                                        </prop:Text>
                                        <prop:ClientSide.OnPreDispatch>                            
                                            Pace.stop();
                                            Pace.start();
                                            $('#<%=$this->btnUpdate->ClientId%>').prop('disabled',true);                      
                                        </prop:ClientSide.OnPreDispatch>
                                        <prop:ClientSide.OnLoading>
                                            $('#<%=$this->btnUpdate->ClientId%>').prop('disabled',true);                                                                      
