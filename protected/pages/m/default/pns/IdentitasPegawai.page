                                        <prop:ClientSide.OnValidationError>
                                            $('#divEditPredikatNUKS').EditClass('has-error');                              
                                        </prop:ClientSide.OnValidationError>
                                        <prop:ClientSide.OnValidationSuccess>
                                            $('#divEditPredikatNUKS').removeClass('has-error');                                 
                                        </prop:ClientSide.OnValidationSuccess>
                                    </com:TRequiredFieldValidator>                                 
                                </div>
                            </div>
                            <div class="form-group" id="divEditTahunSertifikatBerjenjang">
                                <label class="col-md-6 control-label">Tahun Sertifikat Berjenjang</label>
                                <div class="col-md-12">
                                    <com:TDatePicker ID="txtEditSertifikatBerjenjang" DateFormat="yyyy" showCalendar="false" Culture="id" FromYear="1940" UpToYear="<%= @date('Y') %>" InputMode="DropDownList" />
                                </div>
                            </div>                            
                            <div class="form-group" id="divEditNoHP">
                                <label class="col-md-4 control-label">Nomor Handphone</label>
                                <div class="col-md-12">
                                    <com:TTextBox ID="txtEditNoHP" CssClass="form-control" />             
                                    <com:TRequiredFieldValidator ID="FieldEditNoHPValidator" ControlToValidate="txtEditNoHP" Display="Dynamic" ErrorMessage="Isi Atas Nama" ValidationGroup="Editpegawai" FocusOnError="true">
                                        <prop:ClientSide.OnValidationError>
                                            $('#divEditNoHP').EditClass('has-error');                              
                                        </prop:ClientSide.OnValidationError>
                                        <prop:ClientSide.OnValidationSuccess>
                                            $('#divEditNoHP').removeClass('has-error');                                 
                                        </prop:ClientSide.OnValidationSuccess>
                                    </com:TRequiredFieldValidator>                                 
                                </div>
                            </div>
                            <div class="form-group" id="divEditAlamat">
                                <label class="col-md-4 control-label">Alamat Lengkap</label>
                                <div class="col-md-12">
                                    <com:TTextBox ID="txtEditAlamat" CssClass="form-control" Rows="4" TextMode="MultiLine" />             
                                    <com:TRequiredFieldValidator ID="FieldEditAlamatValidator" ControlToValidate="txtEditAlamat"  Display="Dynamic" ErrorMessage="Isi Atas Nama" ValidationGroup="Editpegawai" FocusOnError="true">
